{
  "name": "compare-releases",
  "version": "1.0.0",
  "description": "A GitHub Action which compares releasesa across repos",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc && ncc build -o dist",
    "prettier:write": "npx prettier --write '**/*.{ts,json,md,yaml,yml}'",
    "prettier:check": "npx prettier --check '**/*.{ts,json,md,yaml,yml}'",
    "lint:check": "eslint . --ext .ts",
    "lint:write": "eslint --fix . --ext .ts",
    "test": "jest --coverage",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@octokit/graphql": "^7.0.1",
    "@octokit/rest": "^20.0.1"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.12",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "husky": "^8.0.3",
    "@tsconfig/node18": "^18.2.1"
  },
  "engines": {
    "node": "18"
  }
}
